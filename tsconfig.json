{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@pages/*": ["src/pages/*"],
      "@utils/*": ["src/utils/*"],
      "@hooks/*": ["src/hooks/*"],
      "@types/*": ["src/types/*"],
      // "@assets/*": ["src/assets/*"],
      // "@styles/*": ["src/styles/*"],
      "@api/*": ["src/api/*"],
      "@store/*": ["src/store/*"]
    },
    "ignoreDeprecations": "6.0"
  }
}
